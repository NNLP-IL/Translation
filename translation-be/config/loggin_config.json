{
    "logger_object": {
        "singleton": true,
        "format": "simple",
        "formatters": {
            "simple": "{time:MMMM D, YYYY > HH:mm:ss} | {extra[name]} | {level} | {message}",
            "serialize": true
        },
        "level": "TRACE",
        "filename": "file.log",
        "rotation": {
            "time": "00:00",
            "retention": "3 days"
        }
    }
}